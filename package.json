{
  "name": "clinq-bridge-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate project to bootstrap a new CLINQ Bridge",
  "main": "dist/index.js",
  "repository": "git@github.com:sipgate/clinq-bridge-boilerplate.git",
  "contributors": [
    {
      "name": "Benjamin Kluck",
      "email": "ben@fug.lu"
    },
    {
      "name": "Felix Gladisch",
      "email": "felix@felixgladisch.de"
    }
  ],
  "license": "Apache-2.0",
  "scripts": {
    "postinstall": "tsc",
    "start": "env-cmd -f .env ts-node-dev src/index.ts",
    "lint": "tslint --project tsconfig.json"
  },
  "dependencies": {
    "@clinq/bridge": "^11.3.1",
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.2",
    "@types/google-libphonenumber": "^7.4.21",
    "axios": "^0.21.1",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "typescript": "^4.3.5",
    "google-libphonenumber": "^3.2.22"
  },
  "devDependencies": {
    "husky": "^7.0.1",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "ts-node-dev": "^1.1.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "tslint",
      "git add"
    ],
    "*.{json,yml,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
